{
  "name": "superh-4-asm-syntax",
  "displayName": "SuperH-4 ASM Syntax",
  "description": "SuperH-4 Syntax for .asm files.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "capbilities": {
      "hovesrProvider": true
    },
    "commands": [
      {
        "command": "sh4asm.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "sh4asm.box",
        "title": "Box Selection"
      }
    ],
    "snippets": [
      {
        "language": "sh4asm",
        "path": "./snippets/snippets.code-snippets"
      },
      {
        "language": "sh4asm",
        "path": "./snippets/mvc2_plMemOffsets.code-snippets"
      }
      {
        "language": "sh4asm",
        "path": "./snippets/mvc2_encodedCharIDs.code-snippets"
      }
    ],
    "languages": [
      {
        "id": "sh4asm",
        "aliases": [
          "SH4-ASM",
          "asm",
          "sh4asm"
        ],
        "extensions": [
          ".asm",
          ".sh4asm",
          ".sh4"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "sh4asm",
        "scopeName": "source.sh4asm",
        "path": "./syntaxes/sh4asm.tmLanguage.json"
      }
    ],
    "themes": [
      {
        "id": "SH4ASM vsDark+",
        "label": "SH4 vsDark+",
        "uiTheme": "vs-dark",
        "path": "./themes/sh4asm_darkPlus-themeExt-darkVS.json"
      },
      {
        "id": "SH4ASM vsDark",
        "label": "SH4 vsDark",
        "uiTheme": "vs-dark",
        "path": "./themes/sh4asm_darkVs-themeBase.json"
      },
      {
        "label": ".sh4asm",
        "uiTheme": "vs-dark",
        "path": "./themes/sh4asm-color-theme.json"
      }
    ],
    "iconThemes": [
      {
        "id": "vs-minimal",
        "label": "%minimalIconThemeLabel%",
        "path": "./fileicons/vs_minimal-icon-theme.json"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/magnetro1/sh4asm.git"
  },
  "devDependencies": {
    "@types/vscode": "^1.79.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}